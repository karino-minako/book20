<%= render 'books/list', book: @book, user: @user %>
<h3>Books</h3>
<table>
<thead>
<tr>
  <th></th>
  <th>title</th>
  <th>body</th>
</tr>
</thead>
<tbody>
<% @books.each do |book| %>
<tr>
<td>
<%= attachment_image_tag @user, :profile_image %>
<td>
<%= book.title %>
</td>
<td>
<%= book.body %>
</td>
</tr>  
<% end %>
</tbody>   
</table>