<%= render 'books/list', book: @book, user: @user %>
<table>
<thead>
<tr>
  <th></th>
  <th>title</th>
  <th>body</th>
</tr>
</thead>
<tbody>
<% @books.each do |book| %>
<tr>
<td>
<%= link_to book_path(book.id) do %>
  <%= attachment_image_tag book, :image %>
<% end %>
<td>
<%= book.title %>
</td>
<td>
<%= book.body %>
</td>
</tr>  
<% end %>
</tbody>   
</table>